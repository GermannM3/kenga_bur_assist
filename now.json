{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python@3.1.32", "config": { "runtime": "python3.11" } }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/index.py" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/api/telegram-webhook", "dest": "/api/telegram-webhook.py" }
  ]
} 